{
  "compilerOptions": {
    "target": "ES2024",
    "module": "Node16",
    "moduleResolution": "Node16",
    "lib": ["ES2024"],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "composite": true
  },
  "references": [
    { "path": "packages/core" },
    { "path": "packages/cli" },
    { "path": "packages/server" },
    { "path": "packages/connector-discord" }
  ],
  "files": []
}
